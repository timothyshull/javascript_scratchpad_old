{
  "name": "ng-2-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-express": "node ./bin/www",
    "start-ng": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install && webdriver-manager update",
    "start": "gulp",
    "build": "gulp build",
    "watch": "gulp watch",
    "clean": "gulp clean",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start --single-run",
    "tdd": "karma start",
    "e2e": "npm run build && protractor"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.11",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "serve-favicon": "~2.3.0",
    "systemjs": "0.19.24",
    "zone.js": "0.6.4"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "eslint": "^2.4.0",
    "gulp": "^3.9.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "lite-server": "^2.1.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.15",
    "karma-browserify": "^5.0.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-source-map-support": "^1.1.0",
    "phantomjs-prebuilt": "^2.1.3",
    "protractor": "^3.0.0",
    "typescript": "^1.8.9",
    "typescript-eslint-parser": "0.1.0-alpha-1",
    "typings": "^0.7.9",
    "watchify": "^3.7.0"
  }
}